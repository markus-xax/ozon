    <script>
        let option = {}
        document.addEventListener('DOMContentLoaded', () => {
            option = {...getOptions()}
            option['columnDefs'] = [
                {
                    headerName: 'Запрос',
                    field: 'word',
                    minWidth: 200,
                    cellRenderer: params => `<a href="https://wildberries.ru/catalog/0/search.aspx?search=${params.value}&amp;xsearch=true" target="_blank" title="Открыть в поиске на Wildberries" class="mr-1"> <svg viewBox="0 0 16 16" width="1em" height="1em" focusable="false" role="img" aria-label="search" xmlns="http://www.w3.org/2000/svg" fill="currentColor" class="bi-search b-icon bi"> <g> <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z"></path> </g> </svg> </a> <span>${params.value}</span>`
                },
                {
                    headerName: 'Частота',
                    field: 'count',
                },
                {
                    headerName: 'Индекс',
                    field: 'index_fields',
                },
                {
                    headerName: 'Есть',
                    field: 'count',
                },
                {
                    headerName: 'Бренд',
                    field: 'brand',
                },
                {
                    headerName: 'Название',
                    field: 'name',
                    cellRenderer: params => `-`
                },
                {
                    headerName: 'Цвет',
                    field: 'color',
                    cellRenderer: params => `-`
                },
                {
                    headerName: 'Состав',
                    field: 'sostab',
                    cellRenderer: params => `-`
                },
                {
                    headerName: 'Описание',
                    field: 'description',
                    cellRenderer: params => `-`
                },
                {
                    headerName: 'Параметры',
                    field: 'params',
                    cellRenderer: params => `-`
                },
                {
                    headerName: 'Комплектация',
                    field: 'equipment',
                },
                {
                    headerName: 'Отзывы',
                    field: 'comments',
                },
                {
                    headerName: 'Ответы на отзывы',
                    field: 'comments_answers',
                },
                {
                    headerName: 'Вопросы',
                    field: 'questions',
                },
                {
                    headerName: 'Ответы на вопросы',
                    field: 'questions_answers',
                },
            ]
            option['rowData'] = [
                {% for res in sku.words ?? [] %}
                {
                    word: '{{ res.word??'-' }}',
                    keys_count_sum: '{{ res.keys_count_sum??'-' }}',
                    index_fields: '{{ res.index_fieldsd??'-' }}',
                    count: '{{ res.count??'-' }}',
                    brand: '-',
                    equipment: '{{ res.equipment}}',
                    comments: '{{res.comments}}',
                    comments_answers: '{{res.comments_answers}}',
                    questions: '{{res.questions}}',
                    questions_answers: '{{res.questions_answers}}'
                },
                {% endfor %}
            ]
            new agGrid.Grid(document.getElementsByClassName("ag-theme-material")[0], option)
        })

        function view(bool) {
            document.getElementById('view').style.display = bool ? "none" : "block"
            document.getElementById('close').style.display = bool ? "block" : "none"
        }
    </script>
